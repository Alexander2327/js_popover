{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "rules": {
      "consistent-return": "off",
      "eqeqeq": "off",
      "indent": "off",
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ]
    }
  }